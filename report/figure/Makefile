.PHONY: all_pdf_svg
all_pdf_svg: 

config: 
	ln -s ../config $@


figure: 
	ln -s . $@


.PHONY:      tikz_to_pdf
tikz_to_pdf: out/plots.pdf

out/plots.pdf: plots.tex                                                    \
               config                                                       \
               figure                                                       \
               plot/reference/fig2a-pointer-chasing.tikz                    \
               plot/reference/fig5b-overwrite-delay-p99-latency-sample.tikz
	@latexmk -pdf -outdir=out -quiet plots.tex


